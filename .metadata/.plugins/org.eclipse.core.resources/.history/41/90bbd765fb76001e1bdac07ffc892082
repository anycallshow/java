		System.out.print("1이상의 숫자를 입력하세요 : ");
		int input = sc.nextInt();
		
		for(int i = 1; i <= input; i++) {
			if(input >= 1 ) {
			} else {
				System.out.print("1 이상의 숫자를 입력해주세요.");
			}
		}
		
		
		
	}

}
